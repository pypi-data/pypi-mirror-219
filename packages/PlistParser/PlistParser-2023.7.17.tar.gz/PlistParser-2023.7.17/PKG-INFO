        out_plist = r'plist/example.plist'
        
        plist_parser = PlistParser()  # åˆ›å»ºç©ºè§£æå™¨ plist_parser
        plist_value = plist_parser.parser_dict(plist)  # æ‰§è¡Œplist_parserè§£æå™¨ è§£æ plist ä¸º dict
