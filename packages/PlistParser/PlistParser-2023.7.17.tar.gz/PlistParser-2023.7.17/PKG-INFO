        print(data)  # è®¾ç½®CacheExtra.device_modelçš„å€¼ä¸º 'abbbcd'
        plist_parser.customization_parser_dict_values()
        
        plist_parser = FastPlistParser(plist_parser)
        plist_parser.device_system()  # æŸ¥è¯¢ç³»ç»Ÿåç§°
        plist_parser.device_system('IOS')  # è®¾ç½® è‡ªå®šä¹‰ ç³»ç»Ÿåç§°
