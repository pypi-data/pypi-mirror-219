{
  "name": "jupyterlab-hide-cells",
  "version": "1.0.0",
  "description": "jl-hide-cells",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://git.rwth-aachen.de/learntech-lufgi9/public/jupyterlab-hide-cells",
  "bugs": {
    "url": "https://git.rwth-aachen.de/learntech-lufgi9/public/jupyterlab-hide-cells/issues"
  },
  "license": "MIT License",
  "author": {
    "name": "Annabell Brocker",
    "email": "a.brocker@cs.rwth-aachen.de",
    "url": "https://elearn.rwth-aachen.de/brocker"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://git.rwth-aachen.de/learntech-lufgi9/public/jupyterlab-hide-cells.git"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev",
    "build:prod": "jlpm clean && jlpm build:lib:prod && jlpm build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc --sourceMap",
    "build:lib:prod": "tsc",
    "clean": "jlpm clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "clean:labextension": "rimraf jupyterlab-hide-cells/labextension jupyterlab-hide-cells/_version.py",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
    "eslint": "jlpm eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "install:extension": "jlpm build",
    "lint": "jlpm stylelint && jlpm prettier && jlpm eslint",
    "lint:check": "jlpm stylelint:check && jlpm prettier:check && jlpm eslint:check",
    "prettier": "jlpm prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "jlpm prettier:base --check",
    "stylelint": "jlpm stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "resolutions": {
    "@lumino/widgets": "^1.37.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@jupyterlab/application": "^3.0.0",
    "@jupyterlab/cells": "^3.0.0",
    "@jupyterlab/mainmenu": "^3.0.0",
    "@jupyterlab/notebook": "^3.0.0",
    "@jupyterlab/services": "^6.0.0",
    "@jupyterlab/tooltip": "^3.0.0",
    "@mojs/core": "^1.1.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "typescript": "~4.1.3",
    "@jupyterlab/builder": "^3.6.5"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlab-hide-cells/labextension"
  }
}
